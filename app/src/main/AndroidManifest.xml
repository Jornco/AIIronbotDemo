            android:name=".activity.a7.A7Activity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".activity.a8.A8SearchActivity"
            android:parentActivityName=".MainActivity"/>

        <service
            android:name=".activity.a8.service.A8MyService"
            android:enabled="true"
            android:exported="true">
        </service>

        <activity
            android:name=".activity.a9.A9SearchActivity"
            android:parentActivityName=".MainActivity"/>

        <service
            android:name=".activity.a9.service.A9MyService"
            android:enabled="true"
